#include<stdio.h>
int main()
{
  int a,b;
  //Get values from user
  printf("Enter two numbers: ");
  scanf("%d %d",&a,&b); 
  //print values that user entered
  printf("Before Swapping \n"); 
  printf("a=%d",a);
  printf("\n");
  printf("b=%d",b);
  printf("\n");
  //Swapping values
  printf("After swapping \n");
  a=a+b; 
  b=a-b;
  a=a-b;
  printf("a= %d",a);
  printf("\n");
  printf("b= %d",b);
  printf("\n");
  return 0;
}
